{
    "apps": {
        "http": {
            "servers": {
                "test": {
                    "listen": [":80"],
                    "routes": [
                        {
                            "handle": [
                                {
                                    "handler": "ens",
                                    "eth_rpc_endpoint": "{env.ETH_RPC_ENDPOINT}",
                                    "domain": "offchainexample.eth",
                                    "attributes": [ "address", "contenthash", "public_key", "resolver_address" ]
                                }
                            ]
                        }
                    ]
                }
            }
        }
    },
    "logging": {
        "logs": {
            "default": {
                "level": "DEBUG"
            }
        }
    }
}
